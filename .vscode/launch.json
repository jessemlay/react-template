{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "Chrome",
         "type": "chrome",
         "request": "launch",
         "url": "http://localhost:3000",
         "webRoot": "${workspaceRoot}/src",
         "userDataDir": "${workspaceRoot}/.vscode/chrome",
         "sourceMapPathOverrides": {
            "webpack:///src/*": "${webRoot}/*"
         },
         "breakOnLoad": true,
         "skipFiles": [
            "${workspaceRoot}/node_modules/**/*.js",
            "<node_internals>/**/*.js"
         ],
         "runtimeArgs": ["-r", "flow-remove-types/register", "babel-register"]
      }
   ]
}
